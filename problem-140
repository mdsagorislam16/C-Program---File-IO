#include <stdio.h>
#include <string.h>

int main() {
    char pattern[50], line[100];
    scanf("%s", pattern);

    FILE *f = fopen("data.txt","r");
    if(!f) return 1;

    while(fgets(line,sizeof(line),f)) {
        if(strstr(line,pattern)!=NULL)
            printf("%s", line);
    }

    fclose(f);
    return 0;
}

/*
Input:
apple

Output (if file contains):
I like apple pie
apple is tasty
*/

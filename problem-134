#include <stdio.h>

int main() {
    FILE *fp = tmpfile(); // creates temporary file
    if (fp == NULL) {
        printf("Unable to create temp file\n");
        return 1;
    }

    fprintf(fp, "Hello Temporary File\n"); // write
    fseek(fp, 0, SEEK_SET); // rewind

    char str[50];
    fgets(str, sizeof(str), fp);
    printf("%s", str);

    fclose(fp);
    return 0;
}

/*
Output:
Hello Temporary File
*/
